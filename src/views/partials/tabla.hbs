
<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Precio</th>
            <th scope="col">Foto</th>
        </tr>
    </thead>
           
    <tbody>
            {{#each productos}}
                <tr>
                    <td>{{this.nombre}}</td>
                    <td>$ {{this.precio}}.00</td>
                    <td><img width="50" src="{{this.url}}" alt="not found"></td>
                </tr>
            {{/each}}    
    </tbody

    